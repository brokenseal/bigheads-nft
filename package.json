{
  "name": "bigheads-nft-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:ts": "tsc -p .",
    "build:css": "npx tailwindcss -i ./src/tailwind.css -o ./public/tailwind.css --watch",
    "build:ts:watch": "npm run build:ts -- --watch",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "create-types": "typechain ./src/contracts/**/*.json --target=truffle-v5",
    "clean": "rm -rf ./types/truffle-contracts build ./src/contracts ./src/bigheads/generated"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@bigheads/core": "^0.3.3",
    "@heroicons/react": "^2.0.12",
    "@metamask/providers": "^10.0.0",
    "@types/jsdom": "^20.0.0",
    "jsdom": "^20.0.0",
    "mersenne": "^0.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "^4.0.3",
    "web3": "^1.7.3",
    "weighted": "^1.0.0"
  },
  "devDependencies": {
    "@openzeppelin/contracts": "^4.6.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@typechain/truffle-v5": "^8.0.1",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^10.0.0",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "solc": "^0.8.17",
    "solidity-coverage": "^0.8.0-beta.1",
    "tailwindcss": "^3.2.1",
    "truffle": "^5.5.18",
    "ts-node": "^10.8.1",
    "typechain": "^8.1.0",
    "typescript": "^4.7.4",
    "yargs": "^17.5.1"
  }
}
